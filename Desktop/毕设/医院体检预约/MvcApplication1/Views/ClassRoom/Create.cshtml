@model DBA.Model.ClassRoom
@{
    ViewBag.Title = "Create";
    Layout = null;
}


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>入库</title>
<link href="/Content/css/style.css" rel="stylesheet" type="text/css" />
</head>

<body class="add_test_img"  >

	<div class="place">
    <span>位置：</span>
    <ul class="placeul">
    <li><a href="#">首页</a></li>
    <li><a href="#">添加教室</a></li>
    </ul>
    </div>
    
    <div class="formbody">
    
    <div class="formtitle"><span>添加教室</span></div>
    <form method="post" action="/ClassRoom/Edit">
    <ul class="forminfo">
          <li><label>教室</label>@Html.TextBoxFor(m => m.RoomName, new { @class = "dfinput" })</li>
         <li><label>容纳人数</label>@Html.TextBoxFor(m => m.Num, new { @class = "dfinput" })</li>
         <li><label>空闲时间</label>@Html.TextBoxFor(m => m.KXTime, new { @class = "dfinput" })</li>
         <li><label>教室设备</label>@Html.TextBoxFor(m => m.JSSB, new { @class = "dfinput" })</li>
        @Html.HiddenFor(m=>m.ID)
    <li><label>&nbsp;</label><input name="" style="border:0;" type="submit" class="btn" value="确认保存"/></li>
    </ul>
    </form>
    
    </div>


</body>

</html>



