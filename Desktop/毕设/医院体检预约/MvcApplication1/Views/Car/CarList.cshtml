@model List<DBA.Model.Car>


    <table class="tablelist">
    	<thead>
    	<tr>
        <th>序号<i class="sort"><img src="/Content/images/px.gif" /></i></th>
        <th>编号</th>
           <th>类型</th> 
            <th>制造公司</th> 
             <th>基本维护费</th> 
             <th>总公里数</th> 
            <th>耗油量/公里</th> 
             <th>养路费</th> 
             <th>累计总费用</th> 
            
        <th>操作</th>
        </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
{
    int i = 0;
    DBA.BLL.Category CBLL = new DBA.BLL.Category();
    DBA.BLL.Company COBLL = new DBA.BLL.Company();
            foreach (var item in Model)
        {
            i++;    
        <tr>
        <td>@i</td>
            <td>@item.CarNum</td>
        <td>@CBLL.GetModel(item.CategoryID).CategoryName</td>
            <td>@COBLL.GetModel(item.ComID).ComName</td>
           
             <td>@item.JBWH</td>
             <td>@item.Tkilometre</td>
                  <td>@item.HYL</td>
                      <td>@item.YLF</td>
                          <td>@item.Tprice</td>
        <td><a href="/Car/Create?ID=@item.ID"   >查看</a> 
          <a href="#" onclick="Del(@item.ID)"   >删除</a> 
           
        </td>
        </tr>    
            }
} 
        </tbody>
    </table>
    


    

