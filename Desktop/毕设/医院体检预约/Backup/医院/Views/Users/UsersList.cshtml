@model MVC.PagedList<DBA.Model.Users>
@using MVC


    <table class="tablelist">
    	<thead>
    	<tr>
       
             <th>用户类型</th>
             <th>账号/工号</th>
      
            
            <th>姓名</th>
            <th>性别</th>
            <th>年龄</th>
        <th>操作</th>
        </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
{
    int i = 0; 
            foreach (var item in Model)
        {
            i++;    
        <tr>
      
             
        <td>@item.UserType</td>
            <td>@item.UserName</td>
          
             <td>@item.Str1</td>
             <td>@item.Sex</td>
            <td>@item.Age</td>
        <td><a href="/Users/Create?ID=@item.ID"   >修改</a> 
            <a href="#" onclick="Del(@item.ID)"   >删除</a> 
           
        </td>
        </tr>    
            }
} 
        </tbody>
    </table>
     <div class="pagination pagination-centered">
    	  @Html.Pager(Model, new PagerOptions { ShowFirstLast = false, PrevPageText = "上一页", NextPageText = "下一页", AlwaysShowFirstLastPageNumber = true }, "Paged", new { action = "UsersManage" })
    </div>


    

